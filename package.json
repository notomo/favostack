{
  "private": true,
  "name": "favostack",
  "type": "module",
  "version": "0.1.6",
  "devDependencies": {
    "typescript": "^5.7.3",
    "vitest": "^3.0.7",
    "wxt": "^0.19.27"
  },
  "scripts": {
    "dev": "wxt",
    "build": "wxt zip",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "submit": "wxt submit --chrome-zip .output/*-chrome.zip",
    "submit:dryrun": "wxt submit --dry-run --chrome-zip .output/*-chrome.zip"
  }
}
